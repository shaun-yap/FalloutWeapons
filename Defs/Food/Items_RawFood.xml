<Defs>

<ThingDef ParentName="PlantFoodRawBase">
			<defName>RawTatos</defName>
			<label>Tatos</label>
			<description>A mutated hybrid between potatoes and tomatoes. It tastes horrible raw, and not much better when cooked, but in the wasteland you can't complain.</description>
			<graphicData>
				<texPath>Things/Crops/Veg/Tato</texPath>
			</graphicData>
			<statBases>
				<MarketValue>1.1</MarketValue>
					<Mass>0.15</Mass>
					<Nutrition>0.2</Nutrition>
			</statBases>
			<ingestible>
				<foodType>VegetableOrFruit</foodType>
			</ingestible>
			<comps>
				<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				</li>
			</comps>
</ThingDef>

<ThingDef ParentName="PlantFoodRawBase">
