     <RecipeDef>
      <defName>CreateScrapBot</defName>
      <label>Create Scrapbot</label>
      <description>Create a brand new Scrapbot unit.</description>
      <jobString>Making Robot</jobString>  
      <allowMixingIngredients>false</allowMixingIngredients>
	  <skillRequirements>
		<Crafting>5</Crafting>
      </skillRequirements>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Plasteel</li>
               </thingDefs>
            </filter>
            <count>25</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Steel</li>
               </thingDefs>
            </filter>
            <count>25</count>
         </li>

         <li>
            <filter>
               <thingDefs>
                  <li>ComponentIndustrial</li>
               </thingDefs>
            </filter>
            <count>4</count>
         </li>
      </ingredients>
      <products>
         <ScrapBotKit>1</ScrapBotKit>
      </products>
	  <researchPrerequisite>ScrapBotResearch</researchPrerequisite>
      <workAmount>4000</workAmount>
	  <unfinishedThingDef>UnfinishedRobot</unfinishedThingDef>
      <workSkill>Crafting</workSkill>
    <effectWorking>Repair</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
	 <recipeUsers>
		<li>TableMachining</li>
      </recipeUsers>
	</RecipeDef>
	
	
