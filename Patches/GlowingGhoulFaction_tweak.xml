<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/pawnGroupMakers/li[1]/options</xpath>  <!-- Outlander "normal" group -->
		<value>
   <!-- Pawnkinds from your race, keep points down low to avoid making your pawns a majority -->
      <!-- The more rare, the lower your points should be -->
					<GlowingGhoul_Villager>0.15</GlowingGhoul_Villager>
					<GlowingGhoul_Town_Guard>0.3</GlowingGhoul_Town_Guard>
					<GlowingGhoul_Grenadier_Destructive>0.025</GlowingGhoul_Grenadier_Destructive>
					<GlowingGhoul_Mercenary_Slasher>.25</GlowingGhoul_Mercenary_Slasher>
					<GlowingGhoul_Mercenary_Gunner>0.3</GlowingGhoul_Mercenary_Gunner>
					<GlowingGhoul_Mercenary_Elite>0.3</GlowingGhoul_Mercenary_Elite>
          <GlowingGhoul_Town_Councilman>0.3</GlowingGhoul_Town_Councilman>
		</value>
	</Operation> 
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/pawnGroupMakers/li[2]/options</xpath>  <!-- Outlander "normal" group -->
		<value>
   <!-- Pawnkinds from your race, keep points down low to avoid making your pawns a majority -->
      <!-- The more rare, the lower your points should be -->
					<GlowingGhoul_Villager>1</GlowingGhoul_Villager>
					<GlowingGhoul_Town_Guard>0.3</GlowingGhoul_Town_Guard>
          <GlowingGhoul_Town_Councilman>0.3</GlowingGhoul_Town_Councilman>
		</value>
	</Operation> 
	
	
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/pawnGroupMakers/li[3]/guards</xpath>  <!-- Outlander Caravan guard group -->
		<value>
			<GlowingGhoul_Town_Guard>0.3</GlowingGhoul_Town_Guard>
			<GlowingGhoul_Town_Councilman>0.3</GlowingGhoul_Town_Councilman>
			<GlowingGhoul_Mercenary_Slasher>0.1</GlowingGhoul_Mercenary_Slasher>
		</value>
	</Operation> 
	
<Operation Class="PatchOperationAdd">
		<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/pawnGroupMakers/li[3]/traders</xpath>  <!-- Outlander Caravan guard group -->
		<value>
          <GlowingGhoul_Town_Trader>0.03</GlowingGhoul_Town_Trader>

		</value>
	</Operation> 
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/pawnGroupMakers/li[4]/options</xpath>  <!-- Outlander base group -->
		<value>
			<GlowingGhoul_Villager>0.1</GlowingGhoul_Villager>
			<GlowingGhoul_Town_Guard>0.3</GlowingGhoul_Town_Guard>
			<GlowingGhoul_Grenadier_Destructive>0.25</GlowingGhoul_Grenadier_Destructive>
			<GlowingGhoul_Mercenary_Slasher>0.25</GlowingGhoul_Mercenary_Slasher>
			<GlowingGhoul_Mercenary_Gunner>0.3</GlowingGhoul_Mercenary_Gunner>
			<GlowingGhoul_Mercenary_Elite>0.3</GlowingGhoul_Mercenary_Elite>
			<GlowingGhoul_Town_Councilman>1</GlowingGhoul_Town_Councilman>
		</value>
	</Operation> 
</Patch>